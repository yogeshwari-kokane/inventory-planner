            <![CDATA[
            SELECT t
            FROM EmailDetails t
            WHERE
                t.stencilId = :stencilId AND
                t.group = :groupName
                  ]]>
        </query>
    </named-query>

    findEmailDetailsByStencilId




</entity-mappings>
